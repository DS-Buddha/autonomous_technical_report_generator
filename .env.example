# Google AI API Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_AI_MODEL=gemini-2.0-flash-exp

# Semantic Scholar API (optional - public API has rate limits)
SEMANTIC_SCHOLAR_API_KEY=

# arXiv Configuration (no key needed)
ARXIV_MAX_RESULTS=20

# LangGraph Configuration
LANGGRAPH_CHECKPOINT_DB=sqlite:///data/checkpoints.db
LANGGRAPH_MAX_ITERATIONS=3

# Vector Store Configuration
FAISS_INDEX_PATH=data/vector_store
EMBEDDING_DIMENSION=768
EMBEDDING_MODEL=models/text-embedding-004

# Code Execution Configuration
CODE_EXECUTION_TIMEOUT=30
MAX_CODE_RETRIES=3

# Output Configuration
REPORTS_OUTPUT_DIR=outputs/reports
ENABLE_LOGGING=true
LOG_LEVEL=INFO

# Quality Thresholds
MIN_QUALITY_SCORE=7.0
MIN_RESEARCH_PAPERS=5
MIN_KEY_FINDINGS=10
MIN_TEST_COVERAGE=80.0

# Performance Settings
MAX_CONCURRENT_SEARCHES=5
REQUEST_TIMEOUT=60
